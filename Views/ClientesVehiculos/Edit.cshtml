@model EstacionamientoMVC.Models.ClienteVehiculo

@{
    ViewData["Title"] = @AliasGUI.Edit;
}



<h4>ClienteVehiculo</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="ClienteId" />
            <input type="hidden" asp-for="VehiculoId" />
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="ResponsablePrincipal" /> @Html.DisplayNameFor(model => model.ResponsablePrincipal)
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="@AliasGUI.Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">@AliasGUI.BackToList</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
